{
  "name": "esm-compat",
  "description": "Check the ES module compatibility of a browser.",
  "version": "0.0.2",
  "type": "module",
  "module": "dist/compat.js",
  "types": "types/compat.d.ts",
  "scripts": {
    "prepublishOnly": "pnpm run build",
